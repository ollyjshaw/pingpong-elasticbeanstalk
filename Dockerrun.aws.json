{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "pinger",
      "image": "ollyjshaw/pinger:1",
      "essential": true,
      "memory": 200,
      "links": [
        "ponger"
      ]
    },
    {
      "name": "ponger",
      "image": "ollyjshaw/ponger:1",
      "essential": true,
      "environment": [
        {
            "name": "PONG_MESSAGE",
            "value": "<< MUCH PIPELINE, SO AMAZE WOW!!"
        }
      ],
      "memory": 400,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ]
    }
  ]
}
