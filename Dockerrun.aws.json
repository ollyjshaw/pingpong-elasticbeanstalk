{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "pinger",
      "image": "ollyjshaw/pinger:1",
      "essential": true,
      "memory": 200,
      "links": [
        "ponger"
      ]
    },
    {
      "name": "ponger",
      "image": "ollyjshaw/ponger",
      "essential": true,
      "memory": 200,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ]
    }
  ]
}
